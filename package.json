{
  "name": "wellpath-frontend",
  "version": "0.1.5",
  "description": "Frontend webapp for WellPath, the Rainbow Devs health tracker application.",
  "repository": "github:Rainbow-Devs/health-tracker-frontend",
  "homepage": "https://github.com/Rainbow-Devs/health-tracker-frontend",
  "bugs": "https://github.com/Rainbow-Devs/health-tracker-frontend/issues",
  "author": {
    "name": "Rainbow Devs",
    "url": "https://github.com/Rainbow-Devs"
  },
  "contributors": [
    {
      "name": "Jacob Gill",
      "url": "https://github.com/jcub7012"
    },
    {
      "name": "Michael Trew",
      "url": "https://github.com/CodeDiversity"
    },
    {
      "name": "Spencer Wohlers",
      "url": "https://github.com/seidior"
    }
  ],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18.16.0 || >=20.2.0"
  },
  "keywords": [],
  "packageManager": "pnpm@8.6.2",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && pnpm lint"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "lint": "pnpm eslint:check && pnpm alex:check",
    "eslint:check": "next lint",
    "eslint:fix": "next lint --fix",
    "alex:check": "alex .",
    "format": "pnpm prettier:overwrite",
    "prettier:check": "prettier --check ./app ./components *.cjs",
    "prettier:overwrite": "prettier --write ./app ./components *.cjs",
    "clean": "rm -rf .next node_modules pnpm-lock.yaml package-lock.json yarn.lock .pnpm-store",
    "git:reset": "git reset --hard HEAD",
    "git:clean": "git clean -d -x -e node_modules -e packages -f",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^13.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ariakit/react": "^0.2.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "alex": "^11.0.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
